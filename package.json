{
  "name": "hspt-vocab-app",
  "version": "1.0.0",
  "description": "HSPT Vocabulary Flashcards with gamification and admin dashboard",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "init-db": "node server/scripts/initDb.js",
    "create-admin": "node server/scripts/createAdmin.js",
    "migrate": "node server/scripts/initDb.js",
    "seed": "node server/scripts/importVocab.js && node server/scripts/createAdmin.js",
    "build": "cd client-new && npm install && npm run build",
    "build:prod": "cd client-new && npm ci && npm run build"
  },
  "keywords": [
    "vocabulary",
    "flashcards",
    "education",
    "gamification"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-pg-simple": "^9.0.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.17.3",
    "helmet": "^7.2.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
